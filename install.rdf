<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>tonequilla@mesquilla.com</em:id>
    <em:name>ToneQuilla</em:name>
    <em:version>1.1.1</em:version>
    <em:creator>R Kent James</em:creator>
    <em:description>Play sound as mail filter action</em:description>
    <em:homepageURL>http://www.mesquilla.com/extensions/tonequilla</em:homepageURL>
    <em:unpack>true</em:unpack>
    <!-- Thunderbird -->
